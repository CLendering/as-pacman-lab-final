    "version": "0.2.0",
    "configurations": [
        {
            "name": "Default Pacman Contest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/capture.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "-r",
                "myTeam",
            ]
        },
        {
            "name": "[alternate map, fixed seed] Pacman Contest ",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/capture.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "-r",
                "myTeam",
                "-l",
                "myTestCapture",
                "--fixRandomSeed"
            ]
        },
        {
            "name": "[quiet] Pacman Contest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/capture.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/pacman-contest/src/contest",
            "justMyCode": true,
            "args": [
                "-r",
                "agents/team_with_filter/particleFilterTeam",
                "-q"
            ]
        },
        {
            "name": "Plot Particle Filter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/particle_filter/plot.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "[short, quiet] Pacman Contest with Particle Filter Team",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/capture.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "-r",
                "agents/team_with_filter/particleFilterTeam",
                "-q",
                "-i",
                "20"
            ]
        },
        {
            "name": "Keyboard Agent (doesn't work bc of some bug)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pacman-contest/src/contest/capture.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "--keys0",
            ]
        },
    ]
}